{
  "name": "vue3-ts-vite-ssr-starter",
  "version": "2.0.2",
  "description": "Use vue3 vite typescript eslint SSR vuex vue-router element-plus scss",
  "author": "vok123",
  "scripts": {
    "dev:spa": "vite --force",
    "dev": "node server",
    "build": "yarn build:client && yarn build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "serve": "cross-env NODE_ENV=production node server",
    "preview": "yarn build && yarn serve",
    "lint": "eslint --ext .js,.vue,.ts .",
    "lint:eslint": "eslint --cache --fix --max-warnings 0 --ext .js,.vue,.ts ./src"
  },
  "dependencies": {
    "@feugene/mu": "^4.0.7",
    "@vueuse/core": "^9.1.0",
    "@vueuse/head": "^0.7.9",
    "axios": "^0.27.2",
    "dayjs": "^1.11.4",
    "element-plus": "^2.2.12",
    "pinia": "^2.0.17",
    "vue": "^3.2.37",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.26.1",
    "@iconify/json": "^2.1.89",
    "@intlify/vite-plugin-vue-i18n": "^6.0.0",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/node": "^18.6.5",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@unocss/preset-mini": "^0.45.5",
    "@unocss/preset-web-fonts": "^0.45.5",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/server-renderer": "^3.2.37",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.21.0",
    "eslint-plugin-vue": "^9.3.0",
    "express": "^4.18.1",
    "mockjs": "^1.1.0",
    "rollup": "^2.77.2",
    "rollup-plugin-visualizer": "^5.7.1",
    "sass": "^1.54.4",
    "typescript": "4.7.4",
    "unocss": "^0.45.5",
    "unplugin-auto-import": "^0.11.1",
    "unplugin-icons": "^0.14.8",
    "unplugin-vue-components": "^0.22.4",
    "vite": "^3.0.5",
    "vite-plugin-eslint": "^1.7.0",
    "vite-plugin-inspect": "^0.6.0",
    "vite-plugin-md": "^0.20.2",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-pages": "^0.25.0",
    "vite-plugin-vue-layouts": "^0.7.0",
    "vue-eslint-parser": "^9.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/efureev/kb-front.git"
  }
}
