{
  "name": "vue3-ts-vite-ssr-starter",
  "version": "2.0.2",
  "description": "Use vue3 vite typescript eslint SSR vuex vue-router element-plus scss",
  "author": "vok123",
  "scripts": {
    "dev:spa": "vite --port 1337 --force",
    "dev": "node server",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.js --outDir dist/server",
    "serve": "cross-env NODE_ENV=production node server",
    "preview": "npm run build && npm run serve",
    "lint:eslint": "eslint --cache --fix --max-warnings 0 --ext .js,.vue,.ts ./src"
  },
  "dependencies": {
    "@vueuse/core": "^8.4.2",
    "@vueuse/head": "^0.7.6",
    "axios": "^0.27.2",
    "element-plus": "^2.2.0",
    "pinia": "^2.0.14",
    "vue": "^3.2.33",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@iconify/json": "^2.1.43",
    "@intlify/vite-plugin-vue-i18n": "^3.4.0",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/node": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@unocss/preset-mini": "^0.33.4",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/compiler-sfc": "^3.2.33",
    "@vue/server-renderer": "^3.2.33",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.15.0",
    "eslint-plugin-vue": "^8.7.1",
    "express": "^4.18.1",
    "rollup": "^2.73.0",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.51.0",
    "typescript": "4.6.4",
    "unocss": "^0.33.4",
    "unplugin-icons": "^0.14.3",
    "unplugin-auto-import": "^0.7.1",
    "unplugin-vue-components": "^0.19.5",
    "vite": "^2.9.9",
    "vite-plugin-md": "^0.13.1",
    "vite-plugin-pages": "^0.23.0",
    "vite-plugin-vue-layouts": "^0.6.0",
    "vite-plugin-eslint": "^1.6.0",
    "vue-eslint-parser": "^8.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vok123/vue3-ts-vite-ssr-starter.git"
  },
  "license": "MIT"
}
