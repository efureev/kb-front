{
  "name": "vue3-ts-vite-ssr-starter",
  "version": "2.0.2",
  "description": "Use vue3 vite typescript eslint SSR vuex vue-router element-plus scss",
  "author": "vok123",
  "scripts": {
    "dev:spa": "vite --port 1337 --force",
    "dev": "node server",
    "build": "yarn build:client && yarn build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.js --outDir dist/server",
    "serve": "cross-env NODE_ENV=production node server",
    "preview": "yarn build && yarn serve",
    "lint:eslint": "eslint --cache --fix --max-warnings 0 --ext .js,.vue,.ts ./src"
  },
  "dependencies": {
    "@vueuse/core": "^8.9.4",
    "@vueuse/head": "^0.7.6",
    "axios": "^0.27.2",
    "element-plus": "^2.2.9",
    "pinia": "^2.0.16",
    "vue": "^3.2.37",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.1.2"
  },
  "devDependencies": {
    "@iconify/json": "^2.1.80",
    "@intlify/vite-plugin-vue-i18n": "^5.0.0",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@unocss/preset-mini": "^0.44.5",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/server-renderer": "^3.2.37",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "eslint-plugin-vue": "^9.2.0",
    "express": "^4.18.1",
    "rollup": "^2.77.0",
    "rollup-plugin-visualizer": "^5.7.1",
    "sass": "^1.53.0",
    "typescript": "4.7.4",
    "unocss": "^0.44.5",
    "unplugin-icons": "^0.14.7",
    "unplugin-auto-import": "^0.9.3",
    "unplugin-vue-components": "^0.21.1",
    "vite": "^3.0.2",
    "vite-plugin-md": "^0.18.1",
    "vite-plugin-pages": "^0.25.0",
    "vite-plugin-vue-layouts": "^0.6.0",
    "vite-plugin-eslint": "^1.7.0",
    "vue-eslint-parser": "^9.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vok123/vue3-ts-vite-ssr-starter.git"
  }
}
