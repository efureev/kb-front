{
  "compilerOptions": {
    "target": "es2020",
    "module": "ESNext",
    "sourceMap": true,
    "strict": true,
    "noUnusedLocals": true,
    "jsx": "preserve",
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "baseUrl": ".",
    "types": [
      "axios",
      "node",
      "vite/client",
      "vite-plugin-pages/client",
      "vite-plugin-vue-layouts/client",
      "element-plus/global",
      "./src/@types/*",
      "./types/*",
      "@feugene/mu"
    ],
    "typeRoots": [
      "."
    ],
    "paths": {
      "@/*": [
        "src/*"
      ],
      "#/*": [
        "types/*"
      ]
    }
  },
  "exclude": [
    "dist",
    "node_modules",
    "tests/server/**/*.ts"
  ]
}
